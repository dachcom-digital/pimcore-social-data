{
    "name": "dachcom-digital/social-data",
    "type": "pimcore-bundle",
    "license": "GPL-3.0-or-later",
    "description": "Pimcore Social Data Bundle",
    "keywords": ["pimcore", "social", "feed", "social wall"],
    "homepage": "https://github.com/dachcom-digital/pimcore-social-data",
    "authors": [
        {
            "name": "DACHCOM.DIGITAL Stefan Hagspiel",
            "email": "shagspiel@dachcom.ch",
            "homepage": "http://www.dachcom.com/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "SocialDataBundle\\": "src/SocialDataBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/TestKernel.php"
        ]
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "SocialDataBundle\\SocialDataBundle"
            ]
        },
         "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "require": {
        "pimcore/pimcore": "^6.6",
        "symfony/symfony": "^4.4",
        "doctrine/orm": "^2.7"
    },
    "require-dev": {
        "codeception/codeception": "~2.4.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "symplify/easy-coding-standard": "^8.0"
    }
}
